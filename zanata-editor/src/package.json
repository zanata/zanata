{
  "name": "zanata-spa",
  "version": "0.0.10",
  "description": "Zanata Single Page Application",
  "homepage": "https://github.com/zanata/zanata-spa",
  "main": "index.js",
  "keywords": [
    "css",
    "rework"
  ],
  "bugs": {
    "url": "https://github.com/zanata/zanata-spa/issues"
  },
  "scripts": {
    "watch": "cd app && webpack-dev-server -d --progress --config webpack.dev.config.js --inline --hot --content-base build/",
    "build": "cd app && webpack --progress --config webpack.prod.config.js",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -s ./app/build",
    "build-storybook": "build-storybook -s ./app/build",
    "spritesheet": "gulp icons",
    "storybook-spritesheet": "gulp storybook-icons"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/zanata/zanata-spa.git"
  },
